{
  "name": "luminex-v4-ultimate",
  "version": "4.0.0",
  "description": "Luminex Complete - Next.js 15 App Router 100% Full Features",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:analyze": "ANALYZE=true prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "test:contract": "hardhat test",
    "test:contract:gas": "REPORT_GAS=true hardhat test",
    "compile": "hardhat compile",
    "compile:contracts": "hardhat compile",
    "deploy": "hardhat run scripts/deploy.js",
    "deploy:local": "hardhat run scripts/deploy-worldchain-local.js --network hardhat",
    "deploy:worldchain": "hardhat run scripts/deploy-worldchain.js --network worldchain",
    "deploy:worldchain:testnet": "hardhat run scripts/deploy-worldchain.js --network worldchainTestnet",
    "deploy:worldchain:guide": "echo 'See docs/guides/Deployment.md for detailed guide'",
    "deploy:check": "hardhat run scripts/check-deployment-config.js --network worldchain",
    "verify": "hardhat verify",
    "setup:contract": "hardhat run scripts/setup-contract.js",
    "deployment:info": "hardhat run scripts/get-deployment-info.js",
    "distributor:set": "node scripts/set-game-reward-distributor.js",
    "distributor:address": "node scripts/get-distributor-address.js",
    "distributor:check": "node scripts/check-contract-owner.js",
    "faucet:check": "node scripts/check-faucet-distributor.js"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "@sentry/nextjs": "^10.23.0",
    "@worldcoin/idkit": "^2.4.2",
    "@worldcoin/minikit-js": "^1.9.8",
    "ethers": "^6.15.0",
    "framer-motion": "^11.11.17",
    "lucide-react": "^0.453.0",
    "next": "^15.5.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-qr-code": "^2.0.18",
    "viem": "^2.38.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.6",
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.2",
    "@nomicfoundation/hardhat-network-helpers": "^1.1.2",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.1.1",
    "@openzeppelin/contracts": "^5.4.0",
    "@playwright/test": "^1.48.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@typechain/ethers-v6": "^0.4.3",
    "@typechain/hardhat": "^8.0.3",
    "@types/chai": "^4.3.20",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.9.0",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "autoprefixer": "^10.4.20",
    "chai": "^4.5.0",
    "hardhat": "^2.27.0",
    "hardhat-gas-reporter": "^1.0.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.47",
    "prisma": "^5.20.0",
    "solidity-coverage": "^0.8.16",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.6.3"
  }
}
